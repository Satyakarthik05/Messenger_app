{
  "expo": {
    "name": "Klicko",
    "slug": "Klicko",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_logo2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.satyakarthik.messenger"
    },
    "web": {
      "favicon": "./assets/app_logo2.png"
    },
    "extra": {
      "eas": {
        "projectId": "487fffe3-2034-45b6-a516-2ae413218138"
      }
    }
  }
}
