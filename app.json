{
  "expo": {
    "name": "Klicko",
    "slug": "klicko",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_logo2.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_logo2.jpg",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.satyakarthik.messenger"
    },
    "web": {
      "favicon": "./assets/app_logo2.jpg"
    },
    "extra": {
      "eas": {
        "projectId": "81c174cf-ae98-4d28-9dee-8646d491cb4a"
      }
    },
    "owner": "satyakarthik"
  }
}
